<?xml version="1.0"?>
<launch>

    <!-- Launches the skills simulation action servers -->
    <node name="DriveSkill" pkg="skill_simulation_server" type="skill_action_server.py" output="screen" args="DriveSkill"></node>
    <node name="DriveEdgesSkill" pkg="skill_simulation_server" type="skill_action_server.py" output="screen" args="DriveEdgesSkill"></node>
    <node name="LocateSkill" pkg="skill_simulation_server" type="skill_action_server.py" output="screen" args="LocateSkill"></node>
    <node name="PickSkill" pkg="skill_simulation_server" type="skill_action_server.py" output="screen" args="PickSkill"></node>
    <node name="PlaceSkill" pkg="skill_simulation_server" type="skill_action_server.py" output="screen" args="PlaceSkill"></node>
    <node name="WaitSkill" pkg="skill_simulation_server" type="skill_action_server.py" output="screen" args="WaitSkill"></node>
    <node name="DockSkill" pkg="skill_simulation_server" type="skill_action_server.py" output="screen" args="DockSkill"></node>
    <node name="ActuateArmSkill" pkg="skill_simulation_server" type="skill_action_server.py" output="screen" args="ActuateArmSkill"></node>
    <node name="MoveArmSkill" pkg="skill_simulation_server" type="skill_action_server.py" output="screen" args="MoveArmSkill"></node>
    <node name="GenericSkill" pkg="skill_simulation_server" type="skill_action_server.py" output="screen" args="GenericSkill"></node>

</launch>
